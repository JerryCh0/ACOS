# Выбор компилятора
CC=gcc
#Флаги компилятора
CFLAGS=-c -Wall

all: Plug-In
	rm -rf *.o ; ./Plug-In
	
Plug-In: plugIn.o printing.o sumAB.o
	$(CC) plugIn.o printing.o sumAB.o -o Plug-In

plugIn.o: plugIn.c
	$(CC) $(CFLAGS) plugIn.c

printing.o: printing.c
	$(CC) $(CFLAGS) printing.c

sumAB.o: sumAB.c
	$(CC) $(CFLAGS) sumAB.c
